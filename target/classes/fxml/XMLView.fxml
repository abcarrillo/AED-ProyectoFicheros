<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="view" prefHeight="558.0" prefWidth="533.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="txtRuta" layoutX="83.0" layoutY="34.0" prefHeight="25.0" prefWidth="432.0" />
      <Label layoutX="14.0" layoutY="38.0" prefHeight="17.0" prefWidth="69.0" text="Ruta actual:" />
      <TextArea fx:id="txtContenido" layoutX="81.0" layoutY="134.0" prefHeight="154.0" prefWidth="432.0" />
      <Label layoutX="14.0" layoutY="134.0" prefHeight="17.0" prefWidth="69.0" text="Contenido:" />
      <Button fx:id="btnVerContenido" layoutX="15.0" layoutY="72.0" mnemonicParsing="false" onAction="#onVerContenidoAction" prefHeight="50.0" prefWidth="78.0" text="Ver contenido" textAlignment="CENTER" wrapText="true" />
      <Button fx:id="btnInsertarContrato" layoutX="30.0" layoutY="417.0" mnemonicParsing="false" onAction="#onInsertarContratoAction" prefHeight="121.0" prefWidth="107.0" text="Insertar contrato" />
      <Button fx:id="btnGuardar" layoutX="190.0" layoutY="333.0" mnemonicParsing="false" onAction="#onGuardarArchivoAction" prefHeight="50.0" prefWidth="107.0" text="Guardar archivo en nueva ruta" textAlignment="CENTER" wrapText="true" />
      <Button fx:id="btnEliminar" layoutX="348.0" layoutY="333.0" mnemonicParsing="false" onAction="#onEliminarAction" prefHeight="50.0" prefWidth="107.0" text="Eliminar un equipo" textAlignment="CENTER" wrapText="true" />
      <TextField fx:id="txtNombre" layoutX="148.0" layoutY="449.0" prefHeight="25.0" prefWidth="364.0" promptText="Nombre de futbolista..." />
      <TextField fx:id="txtFechaInicio" layoutX="148.0" layoutY="481.0" prefHeight="25.0" prefWidth="364.0" promptText="Fecha de inicio del contrato..." />
      <TextField fx:id="txtFechaFin" layoutX="148.0" layoutY="512.0" prefHeight="25.0" prefWidth="364.0" promptText="Fecha de fin del contrato..." />
      <TextField fx:id="txtNombreEquipo" layoutX="148.0" layoutY="417.0" prefHeight="25.0" prefWidth="364.0" promptText="Nombre del equipo..." />
      <Button fx:id="btnCopas" layoutX="41.0" layoutY="333.0" mnemonicParsing="false" onAction="#onModificarCopasAction" prefHeight="50.0" prefWidth="107.0" text="Cambiar copas de un equipo" textAlignment="CENTER" wrapText="true" />
   </children>
</AnchorPane>
